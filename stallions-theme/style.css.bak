/* 1008px MEDIA QUERY */
@media only screen and (max-width: 1008px){
    /* Main Menu/Header */
    .bannerw{
        background: url("assets/img/header-bg.png") no-repeat;
        background-size: cover;
        max-height: 130px;
    }

    ul#mainmenu{
        z-index: 2;
        position: fixed;
        background-color: #bb952e;
        box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
        left: -500px;
        top: 0;
        height: 100%;
        width: 35%;
        margin-top: 0px;
        transition: left 0.5s ease;
    }
    li#mainmenu{
        width:100%;
        border-top:1px solid #333;
        padding:0;
        min-height:60px;
    }
    li#mainmenu > a#mainmenu{
        line-height:60px;
    }
    li#mainmenu:last-child{
        border-bottom:1px solid #333;
    }
    ul#teamdropdown, ul#ticketsdropdown, ul#cantinedropdown, ul#fandropdown, ul#contactdropdown, ul#sponsorshipdropdown{
        width:100%;
        z-index:0;
        position: relative;
        border-bottom:none;
        font-size:16px;
        margin-top:0;
    }
    li#teamdropdown, li#ticketsdropdown, li#cantinedropdown, li#fandropdown, li#contactdropdown, li#sponsorshipdropdown{
        padding: 16px 12px;
        margin:0;
        border-top:1px solid black;
    }
    li#teamdropdown:first-child, li#ticketsdropdown:first-child, li#cantinedropdown:first-child, li#fandropdown:first-child, li#contactdropdown:first-child, li#sponsorshipdropdown:first-child{
        margin-top: 0;
        padding-top:17px;
    }
    li#teamdropdown:last-child, li#ticketsdropdown:last-child, li#cantinedropdown:last-child, li#fandropdown:last-child, li#contactdropdown:last-child, li#sponsorshipdropdown:last-child{
        margin-bottom: 0;
    }
    .teammenu:hover > ul#teamdropdown, .ticketsmenu:hover > ul#ticketsdropdown, .cantinemenu:hover > ul#cantinedropdown, .fanmenu:hover > ul#fandropdown, .contactmenu:hover > ul#contactdropdown,
    .sponsorshipmenu:hover > ul#sponsorshipdropdown{
        display: none;
    }
